#!/usr/bin/env python

Import('env')
Import('env_modules')

sources = [
    'register_types.cpp',
    'duktape/duktape.c',
    'ECMAScript.cpp',
    'utils/binding_helper.cpp',
    'utils/builtin_classes.cpp',
]

env_module = env_modules.Clone()
env_module.Append(CPPPATH=["#modules/ECMAScript"])
env_module.add_source_files(env.modules_sources, sources)
